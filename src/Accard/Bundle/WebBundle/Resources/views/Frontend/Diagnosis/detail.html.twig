<div class = "well">
	<div class="row">
		<div class="col-md-4">
			<strong>Code : </strong>
		</div>
		<div class="col-md-8">
			{{diagnosis.code.code}}
		</div>
	</div>
	<div class="row">
		<div class="col-md-4">
			<strong>Description : </strong>
		</div>
		<div class="col-md-8">
			{{diagnosis.code.description}}
		</div>
	</div>
	<div class="row">
		<div class="col-md-4">
			<strong>Start date : </strong>
		</div>
		<div class="col-md-8">
			{{diagnosis.startDate|date('m-d-Y')}}
		</div>
	</div>
	<div class="row">
		<div class="col-md-4">
			<strong>End date : </strong>
		</div>
		<div class="col-md-8">
			{{diagnosis.endDate|date('m-d-Y')}}
		</div>
	</div>
</div>